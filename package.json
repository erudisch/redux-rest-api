{
  "name": "redux-api-promise-middleware",
  "version": "0.1.0",
  "main": "dist/index.js",
  "description": "Redux middleware for generating promises for API actions.",
  "scripts": {
    "build": "babel src --out-dir dist",
    "build:watch": "npm run build -- --watch",
    "clean": "rm -rf dist",
    "lint": "eslint src test",
    "prepublish": "npm run clean && NODE_ENV=production npm run build && npm run test",
    "test": "npm run lint && ava test",
    "test:watch": "ava test --watch"
  },
  "author": "Mario Pabon <me@mariopabon.com> (https://mariopabon.com/)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/restlessbit/redux-api-promise-middleware.git"
  },
  "keywords": [
    "redux",
    "redux-middleware",
    "middleware",
    "promise",
    "promises",
    "api"
  ],
  "bugs": {
    "url": "https://github.com/restlessbit/redux-api-promise-middleware/issues"
  },
  "homepage": "https://github.com/restlessbit/redux-api-promise-middleware#readme",
  "ava": {
    "require": [
      "babel-register",
      "isomorphic-fetch"
    ]
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "sourceMaps": "inline",
    "env": {
      "production": {
        "presets": [
          "babili"
        ]
      }
    }
  },
  "eslintConfig": {
    "extends": "standard",
    "env": {
      "browser": true,
      "node": true
    }
  },
  "dependencies": {
    "object-assign": "^4.1.0"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-preset-babili": "0.0.2",
    "babel-preset-es2015": "^6.14.0",
    "eslint": "^3.4.0",
    "eslint-config-standard": "^6.0.0",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-standard": "^2.0.0",
    "fetch-mock": "^5.1.1",
    "isomorphic-fetch": "^2.2.1",
    "redux": "^3.6.0",
    "sinon": "^1.17.5"
  }
}
